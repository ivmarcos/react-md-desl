    "babel-eslint": "7.1.1",
    "babel-jest": "18.0.0",
    "babel-loader": "6.2.10",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react-app": "^2.2.0",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-runtime": "^6.20.0",
    "bundle-loader": "^0.5.5",
    "case-sensitive-paths-webpack-plugin": "1.1.4",
    "chalk": "1.1.3",
    "connect-history-api-fallback": "1.3.0",
    "cross-spawn": "4.0.2",
    "css-loader": "^0.28.2",
    "detect-port": "1.1.0",
    "dotenv": "2.0.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-config-react-app": "^0.6.2",
    "eslint-loader": "1.6.0",
    "eslint-plugin-flowtype": "2.21.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.10.0",
    "fs-extra": "0.30.0",
    "html-webpack-plugin": "2.24.0",
    "http-proxy-middleware": "0.17.3",
    "jest": "18.1.0",
    "json-loader": "0.5.4",
    "lodash-webpack-plugin": "^0.11.3",
    "object-assign": "4.1.1",
    "postcss-loader": "1.2.2",
    "promise": "7.1.1",
    "react-dev-utils": "^0.5.2",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.0",
    "url-loader": "0.5.7",
    "webpack": "1.14.0",
    "webpack-bundle-analyzer": "^2.8.2",
    "webpack-dev-server": "1.16.2",
    "webpack-manifest-plugin": "^1.1.0",
    "whatwg-fetch": "2.0.2"
  },
  "scripts": {
    "build-css": "node-sass src/ -o src/",
    "watch-css": "npm run build-css && node-sass src/ -o src/ --watch --recursive",
    "start-js": "node scripts/start.js",
    "lint": "yarn run lint:scripts",
    "lint:dev": "nodemon --exec \"yarn lint\"",
    "lint:fix": "eslint -c .eslintrc --fix src",
    "start:server": "nodemon server --watch server",
    "start": "npm-run-all -p start-js start:server",
    "deploy": "yarn release && gulp deploy",
    "deploy:server": "gulp deploy:server",
    "predeploy": "npm run test && npm run build",
    "upgrade": "yarn upgrade ssh://disem7.intranet.bb.com.br:29418/bb-common.git --prefer-offline",
    "upgrade:server": "cd server && yarn run upgrade",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "build": "node scripts/build.js",
    "test": "mocha",
    "release": "yarn version --new-version patch",
    "preversion": "mocha",
    "version": "git add --all",
    "postversion": "git push"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules|scripts)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
