    "react-autocomplete": "^1.5.3",
    "react-dom": "^15.5.4",
    "react-md": "*",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^4.0.8",
    "react-stepper-horizontal": "^1.0.9",
    "react-virtualized": "^9.8.0",
    "react-virtualized-select": "^3.0.1",
    "redux": "^3.6.0",
    "redux-form": "^6.7.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "webfontloader": "^1.6.27"
  },
  "devDependencies": {
    "@storybook/react": "^3.0.1",
    "annotation-loader": "^0.0.2",
    "autoprefixer": "6.7.2",
